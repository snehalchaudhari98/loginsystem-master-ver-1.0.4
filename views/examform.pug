extends layout

block content
    h1 Certified Exam Conducted
    form(method='POST',action='/exam/examform')
        div.form-group
            label Organized at:
            input.form-control(name='organizer',type='text')
        div.form-group
            label Date:
            input.form-control(name='date',type='date',max=yy + "-" + mm + "-" + dd)
        div.form-group
            label Number of count:
            input.form-control(name='count',type='number')
        div.form-group
            label Subject:
            textarea.form-control(name='subject',type='text')
            label Class:
        div.form-group
            select(name='s4', size=1,multiple='')
                option(value='f') First Year
                option(value='s') Second Year
                option(value='t') Third Year
                option(value='b') Fourth Year
            br
        div.form-group
            label PO:
            input.form-control(name='po',type='text')
        div.form-group
            label PSO:
            input.form-control(name='pso',type='text')
        br
        input.btn-primary(type='submit',value='Submit')
